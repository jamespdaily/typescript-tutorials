// .eslintrc
{
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint", "prettier"],
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "rules": {
        "prettier/prettier": "error",
        "arrow-body-style": "off",
        "prefer-arrow-callback": "off"
    }
}
